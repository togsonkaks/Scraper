modules = ["nodejs-20", "web"]
expertMode = true

[env]
# put any keys you want, or leave empty

run = "npm run replit"

[deployment]
deploymentTarget = "vm"

[agent]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Tagglo Lightweight"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Tagglo Test"

[[workflows.workflow]]
name = "Tagglo Lightweight"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run replit"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow]]
name = "Tagglo Test"
author = "agent"

[workflows.workflow.metadata]
outputType = "vnc"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "./start-electron.sh"

[nix]
