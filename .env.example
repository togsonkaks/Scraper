# Local PostgreSQL Database Connection
# Copy this file to .env and fill in your actual credentials

# Local PostgreSQL (port 5433, database "Tagglo")
DATABASE_URL=postgresql://username:password@localhost:5433/Tagglo

# PostgreSQL Connection Details (for reference)
PGHOST=localhost
PGPORT=5433
PGDATABASE=Tagglo
PGUSER=your_username
PGPASSWORD=your_password
